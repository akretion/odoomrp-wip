<?xml version="1.0" encoding="UTF-8"?>
<odoo>

<record id="view_stock_quant_form" model="ir.ui.view">
    <field name="name">move_wizard.stock.quant.form</field>
    <field name="model">stock.quant</field>
    <field name="inherit_id" ref="stock.view_stock_quant_form"/>
    <field name="arch" type="xml">
        <xpath expr="/form/group" position="before">
            <header>
                <button name="%(action_move_quants)d" type="action" string="Move to Another Location" groups="stock.group_stock_user"/>
            </header>
        </xpath>
    </field>
</record>

</odoo>
